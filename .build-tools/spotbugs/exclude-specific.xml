<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- the following ignore intended exposure of internal representation warnings -->
    <Match>
        <Class name="~.*Entrypoint|.*ControlApiMiddleware|.*ConfigurationWatcher|.*RouterSwitchListener|.*RouterFactory|.*FileConfigProvider|.*DockerContainerProvider|.*DockerContainerServiceImporter|.*KubernetesServiceProvider|.*ProviderAggregator|.*JsonDirectoryConfigStore"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="~.*DockerContainerService"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
    </Match>
    <Match>
        <Class name="~.*DockerContainerServiceImporter"/>
        <Bug pattern="IS2_INCONSISTENT_SYNC"/>
    </Match>

</FindBugsFilter>
