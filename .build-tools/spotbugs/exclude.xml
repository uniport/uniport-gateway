<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="~.*Test$"/>
    </Match>
    <Match>
        <Class name="~.*"/>
        <Bug code="UPM_UNCALLED_PRIVATE_METHOD"/>
    </Match>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="VA_FORMAT_STRING_USES_NEWLINE"/>
    </Match>
</FindBugsFilter>
