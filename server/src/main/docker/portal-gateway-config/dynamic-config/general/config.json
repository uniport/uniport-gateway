{
    "http": {
        "routers": [
            {
                "name": "root",
                "middlewares": ["rootRedirectregex"],
                "rule": "Path('/')",
                "service": "dashboard-proxy"
            }
        ],
        "middlewares": [
            {
                "name": "rootRedirectregex",
                "type": "redirectRegex",
                "options": {
                    "regex": "^/$",
                    "replacement": "/dashboard/"
                }
            }
        ],
        "services": []
    }
}
