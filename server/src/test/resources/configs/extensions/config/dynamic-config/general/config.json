{
    "http": {
        "routers": [
            {
                "name": "root",
                "middlewares": [
                    "header"
                ],
                "rule": "Path('/')",
                "service": "backend"
            }
        ],
        "middlewares": [
            {
                "name": "header",
                "type": "example",
                "options": {
                    "value": "Hello world"
                }
            }
        ],
        "services": [
            {
                "name": "backend",
                "servers": [
                    {
                        "host": "local.uniport.ch",
                        "port": "8081"
                    }
                ]
            }
        ]
    }
}
