{
  "entrypoints": [
    {
      "name": "http8000",
      "port": 8000
    }
  ],
  "applications": [
    {
      "name": "proxy",
      "entrypoint": "http8000",
      "requestSelector": {
        "urlPrefix": "/"
      },
      "provider": "ProxyApplication"
    }
  ],
  "providers": [
    {
      "name": "file",
      "filename": "/etc/portal-gateway/file-config.json"
    }
  ]
}
