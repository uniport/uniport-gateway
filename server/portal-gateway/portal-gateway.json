{
  "entrypoints": [
    {
      "name": "http20000",
      "port": 20000
    }
  ],
  "applications": [
    {
      "name": "proxy",
      "entrypoint": "http20000",
      "requestSelector": {
        "urlPrefix": "/"
      },
      "provider": "ProxyApplication"
    }
  ],
  "providers": [
    {
      "name": "file",
      "filename": "/etc/portal-gateway/dynamic-config/auth/config.json",
      "watch": false
    },
    {
      "name": "docker",
      "exposedByDefault": false,
      "network": "portal-gateway",
      "watch": true
    }
  ]
}
