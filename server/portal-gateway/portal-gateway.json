{
  "entrypoints": [
    {
      "name": "http8000",
      "port": 8000
    }
  ],
  "applications": [
    {
      "name": "proxy",
      "entrypoint": "http8000",
      "requestSelector": {
        "urlPrefix": "/"
      },
      "provider": "ProxyApplication"
    }
  ],
  "providers": [
    {
      "name": "file",
      "filename": "file-config.json",
      "directory": "",
      "watch": false
    },
    {
      "name": "docker",
      "endpoint": "unix:///var/run/docker.sock",
      "watch": true
    }
  ]
}
